    "Password": "nacos",
    "AccessKey": "",
    "SecretKey": "",
    "EndPoint": "sub-domain.aliyun.com:8080",
    "LBStrategy": "WeightRandom", //WeightRandom WeightRoundRobin
    "Metadata": {
      "aa": "bb",
      "cc": "dd"
    }
